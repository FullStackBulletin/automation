{
  "name": "createNewsletterIssue",
  "version": "1.0.0",
  "description": "Serverless lambda to parse and store system prices",
  "main": "handler.js",
  "aws-deployment-type": "lambda",
  "scripts": {
    "test:lint": "eslint src/**/*.js test/**/*.test.js",
    "test:unit": "babel-tap test/**/*.test.js --coverage | tee .test_results.txt | tap-spec",
    "test": "npm run test:lint && npm run test:unit",
    "build:cleanup": "rm -rf build/",
    "build:webpack": "NODE_ENV=production webpack",
    "build:copy-resources": "cp serverless.yml build",
    "build:strip-deps": "package-strip-deps < package.json > build/package.json",
    "build": "npm run build:cleanup && npm run build:webpack && npm run build:copy-resources && npm run build:strip-deps",
    "release:tag": "git tag $npm_package_version && git push --tags",
    "release:deploy": "sls deploy --stage prod"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Plnt9/sls-create-newsletter-issue.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Plnt9/sls-create-newsletter-issue/issues"
  },
  "homepage": "https://github.com/Plnt9/sls-create-newsletter-issue#readme",
  "dependencies": {
    "aws-sdk": "^2.20.0",
    "axios": "^0.15.3",
    "best-scheduled-tweets": "1.2.0",
    "cloudinary": "^1.7.0",
    "fb": "^1.1.1",
    "fullstack-book-of-the-week": "^1.0.2",
    "moment": "^2.17.1",
    "moment-timezone": "^0.5.11",
    "source-map-support": "^0.4.11",
    "tech-quote-of-the-week": "2.1.0",
    "truncate": "^2.0.0",
    "twitter": "^1.7.0",
    "utm-builder": "^0.0.1"
  },
  "devDependencies": {
    "babel-core": "6.18.0",
    "babel-loader": "6.2.7",
    "babel-plugin-transform-async-to-generator": "6.16.0",
    "babel-plugin-transform-es2015-destructuring": "6.18.0",
    "babel-plugin-transform-es2015-function-name": "6.9.0",
    "babel-plugin-transform-es2015-modules-commonjs": "6.18.0",
    "babel-plugin-transform-es2015-parameters": "6.18.0",
    "babel-plugin-transform-es2015-shorthand-properties": "6.18.0",
    "babel-plugin-transform-es2015-spread": "6.8.0",
    "babel-plugin-transform-es2015-sticky-regex": "6.8.0",
    "babel-plugin-transform-es2015-unicode-regex": "6.11.0",
    "babel-plugin-transform-object-rest-spread": "^6.23.0",
    "babel-register": "6.18.0",
    "babel-tap": "5.0.0",
    "babili-webpack-plugin": "0.0.6",
    "clean-webpack-plugin": "0.1.13",
    "eslint": "3.8.1",
    "eslint-config-airbnb": "12.0.0",
    "eslint-plugin-import": "1.16.0",
    "eslint-plugin-jsx-a11y": "2.2.3",
    "eslint-plugin-react": "6.4.1",
    "json-loader": "0.5.4",
    "mock-require": "1.3.0",
    "nock": "8.1.0",
    "package-strip-deps": "1.0.0",
    "rimraf": "^2.5.4",
    "sinon": "1.17.6",
    "tap-spec": "4.1.1",
    "tap-xunit": "1.4.0",
    "webpack": "^2.2.1"
  }
}
